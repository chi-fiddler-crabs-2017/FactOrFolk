<h1>Login</h1>
<% if @errors %>
  <ul>
    <% @errors.each do |error| %>
    <li><%= error %></li>
    <%end %>
  </ul>
<%end %>


<form action="/sessions" method="post">
  <h2>Email</h2>
  <input type="text" name="email"><br>
  <h2> Password</h2>
  <input type="password" name="password"><br>
  <input type ="submit" value="Login">
</form>
